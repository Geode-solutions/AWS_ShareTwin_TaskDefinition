{
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 443,
          "protocol": "tcp",
          "containerPort": 443
        },
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "image": "traefik",
      "name": "Traefik",
      "command": [
        "--api.dashboard=true",
        "--api.insecure=true",
        "--log.level=DEBUG",
        "--providers.ecs=true",
        "--providers.ecs.exposedbydefault=false",
        "--providers.ecs.autoDiscoverClusters=false",
        "--providers.ecs.clusters=Dev_VisualizationCluster",
        "--entrypoints.websecure.address=:443",
        "--serversTransport.insecureSkipVerify=true"
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/Dev_VisualizationTaskDefinition/traefik",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "environment": [
        {
          "name": "FLASK_ENV",
          "value": "development"
        },
        {
          "name": "ID",
          "value": "123456"
        }
      ],
      "mountPoints": [
        {
          "containerPath": "/data",
          "sourceVolume": "data"
        }
      ],
      "image": "ghcr.io/geode-solutions/visualization_geodebackend:develop",
      "name": "GeodeBackEnd1",
      "dockerLabels": {
        "traefik.enable": "true",
        "traefik.http.routers.geode1-router.tls": "true",
        "traefik.http.routers.geode1-router.rule": "PathPrefix(`/geode1`)",
        "traefik.http.routers.geode1-router.entrypoints": "websecure",
        "traefik.http.middlewares.geode1-strip.stripprefix.prefixes": "/geode1",
        "traefik.http.middlewares.geode1-strip.stripprefix.forceSlash": "false",
        "traefik.http.routers.geode1-router.middlewares": "geode1-strip"
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/Dev_VisualizationTaskDefinition/geode",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "environment": [
        {
          "name": "FLASK_ENV",
          "value": "development"
        },
        {
          "name": "ID",
          "value": "123456"
        }
      ],
      "mountPoints": [
        {
          "containerPath": "/data",
          "sourceVolume": "data"
        }
      ],
      "image": "ghcr.io/geode-solutions/visualization_geodebackend2:master",
      "name": "GeodeBackEnd2",
      "dockerLabels": {
        "traefik.enable": "true",
        "traefik.http.routers.geode2-router.tls": "true",
        "traefik.http.routers.geode2-router.rule": "PathPrefix(`/geode2`)",
        "traefik.http.routers.geode2-router.entrypoints": "websecure",
        "traefik.http.middlewares.geode2-strip.stripprefix.prefixes": "/geode2",
        "traefik.http.middlewares.geode2-strip.stripprefix.forceSlash": "false",
        "traefik.http.routers.geode2-router.middlewares": "geode2-strip"
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/Dev_VisualizationTaskDefinition/geode",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "memory": "8192",
  "family": "Dev_VisualizationTaskDefinition",
  "taskRoleArn": "arn:aws:iam::622060531233:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::622060531233:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "cpu": "4096",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "volumes": [
    {
      "name": "data"
    }
  ]
}