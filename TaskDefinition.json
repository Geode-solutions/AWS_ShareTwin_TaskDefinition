{
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 443,
          "protocol": "tcp",
          "containerPort": 443
        },
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "image": "traefik",
      "name": "Traefik",
      "command": [
        "--api.dashboard=true",
        "--api.insecure=true",
        "--log.level=DEBUG",
        "--providers.ecs=true",
        "--providers.ecs.exposedbydefault=false",
        "--providers.ecs.autoDiscoverClusters=false",
        "--providers.ecs.clusters=Dev_VisualizationCluster",
        "--entrypoints.websecure.address=:443",
        "--serversTransport.insecureSkipVerify=true"
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/Dev_VisualizationTaskDefinition/traefik",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "mountPoints": [
        {
          "containerPath": "/data",
          "sourceVolume": "data"
        }
      ],
      "image": "ghcr.io/geode-solutions/visualization_backend:develop",
      "name": "VisualizationContainer",
      "dockerLabels": {
        "traefik.enable": "true",
        "traefik.port": "1234",
        "traefik.http.routers.viewer-router.tls": "true",
        "traefik.http.routers.viewer-router.rule": "PathPrefix(`/viewer`)",
        "traefik.http.routers.viewer-router.entrypoints": "websecure",
        "traefik.http.routers.viewer-router.service": "viewer",
        "traefik.http.middlewares.viewer-strip.stripprefix.prefixes": "/viewer",
        "traefik.http.middlewares.viewer-strip.stripprefix.forceSlash": "false",
        "traefik.http.routers.viewer-router.middlewares": "viewer-strip",
        "traefik.http.services.viewer.loadbalancer.server.port": "1234"
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/Dev_VisualizationTaskDefinition/viewer",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "environment": [
        {
          "name": "FLASK_ENV",
          "value": "development"
        },
        {
          "name": "ID",
          "value": "123456"
        }
      ],
      "mountPoints": [
        {
          "containerPath": "/data",
          "sourceVolume": "data"
        }
      ],
      "image": "ghcr.io/geode-solutions/visualization_geodebackend:develop",
      "name": "GeodeBackEnd",
      "dockerLabels": {
        "traefik.enable": "true",
        "traefik.port": "5000",
        "traefik.http.routers.geode-router.tls": "true",
        "traefik.http.routers.geode-router.rule": "PathPrefix(`/geode`)",
        "traefik.http.routers.geode-router.entrypoints": "websecure",
        "traefik.http.middlewares.geode-strip.stripprefix.prefixes": "/geode",
        "traefik.http.middlewares.geode-strip.stripprefix.forceSlash": "false",
        "traefik.http.routers.geode-router.middlewares": "geode-strip",
        "traefik.http.services.viewer.loadbalancer.server.port": "5000"
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/Dev_VisualizationTaskDefinition/geode",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "memory": "8192",
  "family": "Dev_VisualizationTaskDefinition",
  "taskRoleArn": "arn:aws:iam::622060531233:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::622060531233:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "cpu": "4096",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "volumes": [
    {
      "name": "data"
    }
  ]
}