{
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 443,
          "protocol": "tcp",
          "containerPort": 443
        }
      ],
      "image": "traefik",
      "name": "Traefik",
      "command": [
        "--log.level=DEBUG",
        "--providers.docker=true",
        "--providers.docker.exposedbydefault=false",
        "--entrypoints.web.address=:443"
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/Dev_VisualizationTaskDefinition",
          "awslogs-region": "eu-west-3",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      }
    },
    {
      "mountPoints": [
        {
          "containerPath": "/data",
          "sourceVolume": "data"
        },
        {
          "sourceVolume": "traefikroot",
          "containerPath": "/etc/traefik"
        }
      ],
      "image": "ghcr.io/geode-solutions/visualization_backend:develop",
      "name": "VisualizationContainer",
      "dockerLabels": {
        "traefik.enable": "true",
        "traefik.http.routers.viewer-router.rule": "PathPrefix(`/123456/viewer`)",
        "traefik.http.routers.viewer-router.entrypoints": "web",
        "traefik.http.middlewares.viewer-strip.stripprefix.prefixes": "/123456/viewer",
        "traefik.http.middlewares.viewer-strip.stripprefix.forceSlash": "false",
        "traefik.http.routers.viewer-router.middlewares": "viewer-strip"
      }
    },
    {
      "cpu": 0,
      "environment": [
        {
          "name": "FLASK_ENV",
          "value": "production"
        },
        {
          "name": "ID",
          "value": "123456"
        }
      ],
      "mountPoints": [
        {
          "containerPath": "/data",
          "sourceVolume": "data"
        }
      ],
      "image": "ghcr.io/geode-solutions/visualization_geodebackend:develop",
      "name": "GeodeBackEnd",
      "dockerLabels": {
        "traefik.enable": "true",
        "traefik.http.routers.geode-router.rule": "PathPrefix(`/123456/geode`)",
        "traefik.http.routers.geode-router.entrypoints": "web",
        "traefik.http.middlewares.geode-strip.stripprefix.prefixes": "/123456/geode",
        "traefik.http.middlewares.geode-strip.stripprefix.forceSlash": "false",
        "traefik.http.routers.geode-router.middlewares": "geode-strip"
      }
    }
  ],
  "memory": "8192",
  "family": "Dev_VisualizationTaskDefinition",
  "executionRoleArn": "arn:aws:iam::622060531233:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "cpu": "4096",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "volumes": [
    {
      "name": "data"
    },
    {
      "name": "traefikroot"
    }
  ]
}